includes:
    - phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - src
        - config
        - database
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true
    ignoreErrors:
        # Package config files are published to user's config directory
        # where env() calls are appropriate
        -
            identifier: larastan.noEnvCallsOutsideOfConfig
            path: config/secure-otp.php
